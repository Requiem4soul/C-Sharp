using System;
namespace eee
{
	class MainClass
	{
		public static void Main (string[] args)
		{
			int x, y;
			int flag = 0;
			x = int.Parse (Console.ReadLine());
			y = int.Parse (Console.ReadLine());
			if ((x >= 0) && (x <= 4) && (Math.Abs (y) >= 4)) {
				flag = 1;
			}
			else if ((x <= 0) && (x >= -2) && (Math.Abs (y) >= 2)) {
				flag = 1;
			}
			Console.Write ("{0}", flag);
		}
	}
}
